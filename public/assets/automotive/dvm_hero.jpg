<svg width="1920" height="1080" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0A192F;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#020617;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="purple" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#A78BFA;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8B5CF6;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <rect width="1920" height="1080" fill="url(#bg)"/>
  
  <!-- DVM Strategic Flow Visualization -->
  <g transform="translate(960, 540)">
    <!-- Central Hub -->
    <circle cx="0" cy="0" r="150" fill="url(#purple)" opacity="0.3"/>
    <circle cx="0" cy="0" r="100" fill="url(#purple)" opacity="0.5"/>
    <text x="0" y="15" font-family="Montserrat, sans-serif" font-size="48" font-weight="800" fill="#D4AF37" text-anchor="middle">DVM</text>
    
    <!-- Vector 1: EV Platform -->
    <g transform="rotate(-60)">
      <line x1="0" y1="0" x2="0" y2="-300" stroke="url(#purple)" stroke-width="4" opacity="0.6"/>
      <circle cx="0" cy="-300" r="80" fill="url(#purple)" opacity="0.4"/>
      <text x="0" y="-290" font-family="Inter, sans-serif" font-size="24" fill="#A78BFA" text-anchor="middle" transform="rotate(60, 0, -300)">EV Platform</text>
    </g>
    
    <!-- Vector 2: Charging Infrastructure -->
    <g transform="rotate(0)">
      <line x1="0" y1="0" x2="0" y2="-300" stroke="url(#purple)" stroke-width="4" opacity="0.6"/>
      <circle cx="0" cy="-300" r="80" fill="url(#purple)" opacity="0.4"/>
      <text x="0" y="-290" font-family="Inter, sans-serif" font-size="24" fill="#A78BFA" text-anchor="middle">Infrastructure</text>
    </g>
    
    <!-- Vector 3: Fleet Operations -->
    <g transform="rotate(60)">
      <line x1="0" y1="0" x2="0" y2="-300" stroke="url(#purple)" stroke-width="4" opacity="0.6"/>
      <circle cx="0" cy="-300" r="80" fill="url(#purple)" opacity="0.4"/>
      <text x="0" y="-290" font-family="Inter, sans-serif" font-size="24" fill="#A78BFA" text-anchor="middle" transform="rotate(-60, 0, -300)">Fleet Ops</text>
    </g>
    
    <!-- Vector 4: Policy & Regulation -->
    <g transform="rotate(120)">
      <line x1="0" y1="0" x2="0" y2="-300" stroke="url(#purple)" stroke-width="4" opacity="0.6"/>
      <circle cx="0" cy="-300" r="80" fill="url(#purple)" opacity="0.4"/>
      <text x="0" y="-290" font-family="Inter, sans-serif" font-size="24" fill="#A78BFA" text-anchor="middle" transform="rotate(-120, 0, -300)">Policy</text>
    </g>
    
    <!-- Vector 5: Finance & Investment -->
    <g transform="rotate(180)">
      <line x1="0" y1="0" x2="0" y2="-300" stroke="url(#purple)" stroke-width="4" opacity="0.6"/>
      <circle cx="0" cy="-300" r="80" fill="url(#purple)" opacity="0.4"/>
      <text x="0" y="-290" font-family="Inter, sans-serif" font-size="24" fill="#A78BFA" text-anchor="middle" transform="rotate(-180, 0, -300)">Finance</text>
    </g>
    
    <!-- Vector 6: Aftersales & Service -->
    <g transform="rotate(240)">
      <line x1="0" y1="0" x2="0" y2="-300" stroke="url(#purple)" stroke-width="4" opacity="0.6"/>
      <circle cx="0" cy="-300" r="80" fill="url(#purple)" opacity="0.4"/>
      <text x="0" y="-290" font-family="Inter, sans-serif" font-size="24" fill="#A78BFA" text-anchor="middle" transform="rotate(-240, 0, -300)">Aftersales</text>
    </g>
  </g>
  
  <!-- Title -->
  <text x="960" y="150" font-family="Montserrat, sans-serif" font-size="90" font-weight="800" fill="#A78BFA" text-anchor="middle">DUAL VECTOR MOBILITY</text>
  <text x="960" y="220" font-family="Inter, sans-serif" font-size="40" fill="#D4AF37" text-anchor="middle">Strategic Orchestration Program</text>
  
  <!-- Subtitle -->
  <text x="960" y="950" font-family="Inter, sans-serif" font-size="28" fill="#a8b2d1" text-anchor="middle" opacity="0.7">Comprehensive EV Ecosystem Development</text>
  
  <!-- Grid Pattern -->
  <g opacity="0.08">
    <line x1="0" y1="360" x2="1920" y2="360" stroke="#A78BFA" stroke-width="1"/>
    <line x1="0" y1="720" x2="1920" y2="720" stroke="#A78BFA" stroke-width="1"/>
    <line x1="640" y1="0" x2="640" y2="1080" stroke="#A78BFA" stroke-width="1"/>
    <line x1="1280" y1="0" x2="1280" y2="1080" stroke="#A78BFA" stroke-width="1"/>
  </g>
</svg>
